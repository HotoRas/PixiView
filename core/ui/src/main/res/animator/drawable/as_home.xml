<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/selectOn"
        android:drawable="@drawable/vec_home_on"
        android:state_checked="true" />
    <item
        android:id="@+id/selectOff"
        android:drawable="@drawable/vec_home_off" />
    <transition
        android:drawable="@drawable/av_home_on"
        android:fromId="@id/selectOff"
        android:toId="@id/selectOn" />
    <transition
        android:drawable="@drawable/av_home_off"
        android:fromId="@id/selectOn"
        android:toId="@id/selectOff" />

</animated-selector>